@model IEnumerable<Grupp29.Models.RoomList>

@{
    ViewBag.Title = "Min sida";
}

<h3>Min lista</h3>

<p>
    @Html.ActionLink("Lägg till lista", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.listName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.listName)
        </td>
        <td>
            @Html.ActionLink("Redigera", "Edit", new { id=item.id }) |
            @*@Html.ActionLink("Details", "Details", new { id=item.id })*@ |
            @Html.ActionLink("Ta bort", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
