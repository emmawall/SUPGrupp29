@model Grupp29.Models.ForumPostCategory

@{
    ViewBag.Title = "Delete";
}

<link href="~/Css/StyleManage.css" rel="stylesheet" />

@if (Request.IsAuthenticated)
{

    <h2>Ta bort kategori</h2>

    <h3>Är du säker på att du vill ta bort denna kategori?</h3>
    <div>
        <hr />
        <dl class="dl-horizontal">
        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Ta bort" class="btn btn-default" /> |
                @Html.ActionLink("Tillbaka", "Index")
            </div>
        }
    </div>
}
else
{
    <link href="~/Css/StyleManage.css" rel="stylesheet" />
    <h1>Du måste logga in för att komma åt denna sida!</h1>
}