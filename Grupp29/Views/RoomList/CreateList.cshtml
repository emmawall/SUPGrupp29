@model Grupp29.Models.RoomList
@{
    ViewBag.Title = "Skapa en ny lista";
}

<h2>@ViewBag.Title</h2>
@*<h3>@ViewBag.Message</h3>*@

<br />
@using (Html.BeginForm())
{


    @Html.AntiForgeryToken()


<div class="form-horizontal">

    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

    <div class="form-group">

        @Html.EditorFor(m => m.listName, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(m => m.listName, "", new { @class = "text-danger" })


    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Lägg till kategori" class="btn btn-default" />
        </div>
    </div>

</div>
}
    

    <br />
    <br />


    <div>

        @Html.ActionLink("Tillbaka", "MyPage", "MyPage")

    </div>










